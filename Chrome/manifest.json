{
  "manifest_version": 2,

  "name": "FLEX â€“ Favslist Expansion",
  "short_name": "flex",
  "description": "Customize and expand your Favslist experience.",
  "homepage_url": "https://github.com/WoodrowShigeru/flex",
  "version": "0.3.80",
  "author": "Woodrow Shigeru",
  "icons": {
    "256": "icons/icon_256.png"
  },
  "background": {
    "scripts": [
      "js/handle_woodbox.js",
      "js/prepare_modules.js",
      "js/background.js"
    ],
    "persistent": false
  },
  "permissions": [
    "storage"
  ],
  "options_page": "options.html",

  "content_scripts": [
    {
      "matches": [
        "http://www.favslist.com/*",
        "https://www.favslist.com/*",
        "http://favslist.com/*",
        "https://favslist.com/*"
      ],
      "exclude_matches": [
        "http://*/*.jpg",
        "https://*/*.jpg",
        "http://*/*.jpeg",
        "https://*/*.jpeg",
        "http://*/*.png",
        "https://*/*.png",
        "http://*/*.gif",
        "https://*/*.gif"
      ],
      "js": [
        "js/jquery-1.11.0.js",
        "js/jquery-modifications.js",
        "js/jquery.cyntax.plugin.min.js",
        "js/jquery.cyntax.timer.min.js",
        "js/jquery-ui-1.10.4.custom.min.js",

        "includes/modifyFavsland_forChrome.js",
        "includes/modules.js"
      ],
      "css": ["css/style.css"]
    }
  ],
  "web_accessible_resources": [
      "js/jquery.min.map",
      "img/black-curtain-diag-stroke.png",
      "img/logo-favsland.png",
      "img/pause-play.png"
  ]
}
